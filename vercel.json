{
  "version": 2,
  "builds": [
    {
      "src": "app.ts",
      "use": "@vercel/node@1.9.0"
    },
    {
      "src": "src/bingo/main.py",
      "use": "@vercel/python"
    },
    {
      "src": "src/shootingGame/main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/run",
      "dest": "app.ts",
      "methods": [
        "POST"
      ]
    },
    {
      "src": "/(.*)",
      "dest": "app.ts"
    }
  ]
}